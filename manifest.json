{
  "name": "Midas-3-Extension",
  "description": "Midas-3 chrome extensions.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "css": ["index.css"]
    }
  ],
  "action": {
    "default_title": "Click Me",
    "default_popup": "index.html"
  }
}
